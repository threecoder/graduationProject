<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.camp_android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".usermodel.LoginActivity" android:label="@string/title_activity_login"></activity>
        <activity android:name=".usermodel.UserDetailActivity" android:label="User_info"></activity>
        <activity android:name=".usermodel.MyTrainingActivity" android:label="My_training"></activity>
        <activity android:name=".usermodel.MyActivityActivity" android:label="My_activity"></activity>
        <activity android:name=".usermodel.MyExamActivity" android:label="My_exam"></activity>
        <activity android:name=".usermodel.MyOrderActivity" android:label="My_order"></activity>
        <activity android:name=".usermodel.MyVoteActivity" android:label="My_order"></activity>
        <activity android:name=".usermodel.MyCerActivity" android:label="My_order"></activity>
        <activity android:name=".usermodel.MyMsgActivity" android:label="My_order"></activity>
        <activity android:name=".NewsDetailActivity" android:label="News_detail"></activity>
        <activity android:name=".TrainingDetailActivity" android:label="Training_detail"></activity>
        <activity android:name=".ActivityDetailActivity" android:label="Activity_detail"></activity>
        <activity android:name=".VoteDetailActivity" android:label="Activity_detail"></activity>
        <activity android:name=".VoteStaticDetailActivity" android:label="Activity_detail"></activity>
        <activity android:name=".MsgDetailActivity" android:label="Activity_detail"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

</manifest>